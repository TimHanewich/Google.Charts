<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.1.0</Version>
    <Authors>Tim Hanewich</Authors>
    <Description>Lightweight library for interfacing with Google's Image Chart API</Description>
    <Copyright>Tim Hanewich</Copyright>
    <PackageLicenseFile>License.md</PackageLicenseFile>
    <RepositoryUrl>https://github.com/TimHanewich/TimHanewich.Google.Charts</RepositoryUrl>
    <PackageTags>google,charts,google charts,google api,api</PackageTags>
    <AssemblyVersion>0.0.0.0</AssemblyVersion>
    <PackageIcon>google_icon.png</PackageIcon>
    <FileVersion>0.0.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="google_icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="License.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
